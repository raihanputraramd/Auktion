<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.auktion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserActivity.UserProfileActivity"></activity>
        <activity android:name=".UserActivity.UserDetailItemActivity" />
        <activity android:name=".UserActivity.UserMenuActivity" />
        <activity android:name=".StaticActivity.RegisterOrLoginActivity" />
        <activity android:name=".AdminRegisterActivity" />
        <activity android:name=".UserActivity.UserActivityRegister" />
        <activity android:name=".StaticActivity.AppIntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>